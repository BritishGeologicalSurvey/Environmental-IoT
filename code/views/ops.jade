            .box-header
              // tools box
              .pull-right.box-tools
              // /. tools
            .col-xs-6.text-center(style='border-right: 1px solid #f4f4f4')
              input.knob(type='text', data-readonly='true', value='20', data-width='100', data-height='100', data-fgcolor='#39CCCC')
