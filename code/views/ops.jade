body.hold-transition.skin-black-light.sidebar-mini
  .wrapper
    // Main Header
    header.main-header
      // Logo
      a.logo(href='index2.html')
        // mini logo for sidebar mini 50x50 pixels
        span.logo-mini IOT
        // logo for regular state and mobile devices
        span.logo-lg IOT
      // Header Navbar
      nav.navbar.navbar-static-top(role='navigation')
        // Sidebar toggle button
        a.sidebar-toggle(href='#', data-toggle='offcanvas', role='button')
          span.sr-only Toggle navigation
        // Navbar Right Menu
        .navbar-custom-menu
          ul.nav.navbar-nav
            // Notifications Menu
            li.dropdown.tasks-menu
              // Menu Toggle Button
              a.dropdown-toggle(href='#', data-toggle='dropdown')
                i.fa.fa-flag-o
                span.label.label-danger 9
              ul.dropdown-menu
                li.header You have 9 new notifications
                li
                  // Inner menu: contains the tasks
                  ul.menu
                    li
                      // Task item
                      a(href='#')
                        // Task title and progress text
                        h3
                          | Sensor Battery Life
                          small.pull-right 20%
                        // The progress bar
                        .progress.xs
                          // Change the css width attribute to simulate progress
                          .progress-bar.progress-bar-aqua(style='width: 20%', role='progressbar', aria-valuenow='20', aria-valuemin='0', aria-valuemax='100')
                            span.sr-only 20% Complete
                    // end task item
                li.footer
                  a(href='#') View all notifications
            // User Account Menu
            li.dropdown.user.user-menu
              // Menu Toggle Button
              a.dropdown-toggle(href='#', data-toggle='dropdown')
                // The user image in the navbar
                img.user-image(src='/img/boxed-bg.jpg', alt='User Image')
                // hidden-xs hides the username on small devices so only the image appears.
                span.hidden-xs Dashboard Admin
              ul.dropdown-menu
                // The user image in the menu
                li.user-header
                  img.img-circle(src='/img/boxed-bg.jpg', alt='User Image')
                  p
                    | IoT Dashboard Admin
                    small Member since Nov. 2016
                // Menu Body
                li.user-body
                  .col-xs-4.text-center
                    a(href='#') Value
                  .col-xs-4.text-center
                    a(href='#') Value
                  .col-xs-4.text-center
                    a(href='#') Value
                // Menu Footer
                li.user-footer
                  .pull-left
                    a.btn.btn-default.btn-flat(href='#') Profile
                  .pull-right
                    a.btn.btn-default.btn-flat(href='#') Sign out
    // Left side column. contains the logo and sidebar
    aside.main-sidebar
      // sidebar: style can be found in sidebar.less
      section.sidebar
        // Sidebar Menu
        ul.sidebar-menu
          li.header IOT DASHBOARD SECTIONS
          // Optionally, you can add icons to the links
          li.active
            a(href='/ops')
              i.fa.fa-dashboard
              span Operations
          li
            a(href='/deSoil')
              i.fa.fa-bar-chart
              span Soil Sensors
          li
            a(href='/deSheep')
              i.fa.fa-paw
              span Sheep Sensors
        // /.sidebar-menu
      // /.sidebar
// Content Wrapper. Contains page content
.content-wrapper
  // Content Header (Page header)
  section.content-header
    h1
      | Operations
      small
        | Here you can monitor the hardware and software operations as part of this IoT project.
    ol.breadcrumb
      li
        a(href='#')
          i.fa.fa-dashboard
          |  Operations
      li.active Summary
  // Main content
  section.content
    // Your Page Content Here
    .row
      section.col-lg-7.connectedSortable
        // Small boxes (Stat box)
        .row
          .col-lg-6.col-xs-6
            // small box
            .small-box.bg-primary
              .box-header
                // tools box
                .pull-right.box-tools
                // /. tools
              .inner
                h3 10 / 18
                p Node Status
              .icon
                i.ion.ion-ionic
          // ./col
          .col-lg-6.col-xs-6
            // small box
            .small-box.bg-primary
              .box-header
                // tools box
                .pull-right.box-tools
                // /. tools
              .inner
